		Each defined material requires a distinct "id" -->
		<MATERIAL id="mat1">
			<shininess value="0" />
			<!-- specular reflection -->
			<specular r="0" g="0" b="0" a="0" />
			<!-- diffuse reflection -->
			<diffuse r="0.5" g="0.5" b="0.5" a="1" />
			<!-- ambient reflection -->
			<ambient r="1" g="1" b="1" a="1" />
			<!-- emission component -->
			<emission r="0" g="0" b="0" a="0" />
		</MATERIAL>
		<MATERIAL id="mat2">
			<shininess value="0" />
			<!-- specular reflection -->
			<diffuse r="0.8" g="0.8" b="0.8" a="0.8" />
			<!-- diffuse reflection -->
			<specular r="0" g="0" b="0" a="0" />
			<!-- ambient reflection -->
			<ambient r="0" g="0" b="0" a="0" />
			<!-- emission component -->
			<emission r="0" g="0" b="0" a="0" />
